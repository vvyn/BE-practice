<%- include("partials/header"); %>

<h1> Home </h1>
<p> <%= content.substring(0,100) + "... " %> </p>
<% posts.forEach(function(post) { %>
    <h1> <%= post.itemTitle %> </h1>
    <p> 
        <%= post.itemBody.substring(0,100) + "... " %>
        <a href="/posts/<%=post.itemTitle%>"> Read More</a>
    </p>
<% }); %>  


<%- include("partials/footer"); %>
